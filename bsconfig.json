{
  "name": "votes-web",
  "uncurried": true,
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "suffix": ".mjs",
  "ppx-flags": [["rescript-relay/ppx", "-uncurried"]],
  "namespace": true,
  "bsc-flags": ["-open RescriptStdlib"],
  "bs-dependencies": [
    "@rescript/react",
    "rescript-relay",
    "rescript-relay-router",
    "@gabnor/rescript-stdlib"
  ]
}
