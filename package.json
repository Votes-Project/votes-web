{
  "name": "votes-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "yarn@3.2.1",
  "scripts": {
    "build": "yarn res && vite build",
    "preview": "vite preview",
    "build:graph": "graphclient build --fileType js",
    "dev": "vite",
    "graph:graphiql": "graphclient serve-dev",
    "get-graph-schema": "npx -y get-graphql-schema https://api.studio.thegraph.com/query/9032/votes-goerli/version/latest > schemas/graphschema.graphql",
    "relay": "rescript-relay-compiler",
    "res": "rescript build -with-deps",
    "resgraph": "resgraph build",
    "relay:watch": "rescript-relay-compiler --watch",
    "res:watch": "rescript build -with-deps -w",
    "resgraph:watch": "resgraph watch",
    "codegen": "run-s build:graph relay res resgraph"
  },
  "dependencies": {
    "@glennsl/rescript-fetch": "^0.2.0",
    "@rainbow-me/rainbowkit": "^1.0.2",
    "@remix-run/web-fetch": "^4.3.4",
    "@rescript/core": "^0.4.0",
    "@rescript/react": "0.12.0-alpha.2",
    "framer-motion": "^10.13.0",
    "graphql": "^16.7.1",
    "graphql-yoga": "^4.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-modal-sheet": "^2.0.0",
    "react-relay": "^15.0.0",
    "relay-runtime": "^15.0.0",
    "rescript": "11.0.0-beta.1",
    "rescript-relay": "3.0.0-alpha.1",
    "rescript-relay-router": "^1.0.1",
    "resgraph": "^0.6.2",
    "viem": "^1.1.4",
    "wagmi": "^1.2.1"
  },
  "devDependencies": {
    "@graphprotocol/client-cli": "^3.0.0",
    "@jihchi/vite-plugin-rescript": "^5.3.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitejs/plugin-react-refresh": "1.3.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "tailwindcss": "^3.3.2",
    "vite": "^4.4.3"
  },
  "engines": {
    "node": ">=16"
  }
}
