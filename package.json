{
  "name": "votes-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "yarn@3.2.1",
  "scripts": {
    "build": "run-s 'build:*'",
    "dev": "vite",
    "watch": "run-p 'watch:*'",
    "build:graph": "graphclient build --fileType js",
    "build:relay": "rescript-relay-compiler",
    "build:rescript": "rescript build",
    "build:vite": "vite build",
    "preview": "vite preview",
    "graph:graphiql": "graphclient serve-dev",
    "get-graph-schema": "npx -y get-graphql-schema https://api.studio.thegraph.com/query/9032/votes-goerli/version/latest > schemas/graphschema.graphql",
    "res": "rescript build ",
    "resgraph": "resgraph build",
    "watch:relay": "rescript-relay-compiler --watch",
    "watch:res": "rescript build -w",
    "watch:resgraph": "resgraph watch",
    "watch:vite": "vite",
    "format": "rescript-relay-cli format-all-graphql",
    "rm-unused-fields": "rescript-relay-cli remove-unused-fields"
  },
  "dependencies": {
    "@davatar/react": "^1.11.1",
    "@glennsl/rescript-fetch": "^0.2.0",
    "@rainbow-me/rainbowkit": "^1.0.2",
    "@remix-run/web-fetch": "^4.3.4",
    "@rescript/core": "^0.4.0",
    "@tailwindcss/forms": "^0.5.4",
    "brightid_sdk_v5": "^1.0.2",
    "buffer": "^6.0.3",
    "chart.js": "^4.4.0",
    "framer-motion": "^10.16.3",
    "graphql": "^16.7.1",
    "graphql-yoga": "^4.0.3",
    "jose": "^4.14.4",
    "process": "^0.11.10",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-contenteditable": "^3.3.7",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-modal": "^3.16.1",
    "react-modal-sheet": "^2.0.0",
    "react-relay": "^15.0.0",
    "react-tooltip": "latest",
    "react-vertical-timeline-component": "^3.6.0",
    "relay-runtime": "^15.0.0",
    "rescript": "11.0.0-rc.5",
    "rescript-relay": "3.0.0-alpha.1",
    "rescript-relay-router": "^1.0.1",
    "rescript-struct": "^5.0.1",
    "resgraph": "0.6.3",
    "sanitize-html": "^2.11.0",
    "util": "^0.12.5",
    "viem": "^1.5.0",
    "wagmi": "^1.3.9"
  },
  "devDependencies": {
    "@graphprotocol/client-cli": "^3.0.0",
    "@jihchi/vite-plugin-rescript": "^5.3.0",
    "@rescript/react": "0.12.0-alpha.2",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitejs/plugin-react-refresh": "1.3.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "tailwindcss": "^3.3.2",
    "vite": "^4.4.3"
  },
  "engines": {
    "node": ">=16"
  }
}
