{
  "name": "votes-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "yarn@3.2.1",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "graph:build": "graphclient build --fileType js",
    "graphiql": "graphclient serve-dev",
    "prettier:write": "prettier --write 'src/**/*.{js,ts,tsx,mjs,cjs,json}'",
    "lint": "eslint src --ext ts,tsx,js,jsx,mjs --report-unused-disable-directives --max-warnings 0 && pnpm prettier:write ",
    "preview": "vite preview",
    "graph:schema": "npx -y get-graphql-schema https://api.studio.thegraph.com/query/9032/votes-goerli/version/latest > schemas/graphschema.graphql",
    "relay": "rescript-relay-compiler",
    "relay:watch": "rescript-relay-compiler --watch",
    "res": "rescript build -with-deps",
    "res:watch": "rescript build -with-deps -w",
    "codegen": "yarn graph:build && yarn res && yarn relay"
  },
  "lint-staged": {
    "**/*": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && pnpm prettier:write"
  },
  "dependencies": {
    "@gabnor/rescript-stdlib": "1.0.8",
    "@rainbow-me/rainbowkit": "^1.0.2",
    "@remix-run/web-fetch": "^4.3.4",
    "@rescript/react": "0.12.0-alpha.2",
    "graphql": "^16.6.0",
    "init": "^0.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-relay": "^15.0.0",
    "relay-runtime": "^15.0.0",
    "rescript-relay": "0.0.0-version-3-8b552902",
    "rescript-relay-router": "^1.0.0",
    "viem": "^1.1.4",
    "wagmi": "^1.2.1"
  },
  "devDependencies": {
    "@graphprotocol/client-cli": "^3.0.0",
    "@jihchi/vite-plugin-rescript": "^5.3.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "rescript": "11.0.0-beta.2",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.2"
  }
}
